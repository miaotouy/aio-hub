{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "AIO-Hub",
  "version": "0.1.1-m-beta.1",
  "identifier": "com.aiohub.mobile",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1430",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "aiohub-mobile",
        "width": 800,
        "height": 600,
        "backgroundColor": "#121212"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
