  "workspaces": [
    "plugins/*",
    "mobile"
  ],
  "scripts": {
    "dev": "vite",
    "download:libs": "bun scripts/download-libs.ts",
    "build": "bun run download:libs ; cross-env NODE_OPTIONS=--max-old-space-size=8192 vue-tsc --noEmit ; cross-env NODE_OPTIONS=--max-old-space-size=8192 vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "build:portable": "bun scripts/build-portable.ts",
    "b:p": "bun scripts/build-portable.ts",
    "t:d": "tauri dev",
    "t:b": "tauri build",
    "mtad": "cd mobile ; bun run tauri android dev",
    "mtab": "cd mobile ; bun run tauri android build",
    "mtid": "cd mobile ; bun run tauri ios dev",
    "mtib": "cd mobile ; bun run tauri ios build",
    "check": "bun run check:frontend ; bun run check:backend ; bun run check:mobile",
    "check:frontend": "vue-tsc --noEmit",
    "check:backend": "cargo clippy --manifest-path src-tauri/Cargo.toml -- -D warnings",
    "check:mobile": "cd mobile ; bun run check",
    "check:mobile:frontend": "cd mobile ; bun run check:frontend",
    "check:mobile:backend": "cd mobile ; bun run check:backend"
  },
